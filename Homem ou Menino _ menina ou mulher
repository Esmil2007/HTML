<script>



    $sexo = prompt("Qual seu sexo?")

    $idade = prompt("Qual sua idade?")


    if ($sexo == "m") {
        if ($idade < 18) { alert("Menino") }

        else {
            alert("Homem")
        }
    }

    else {
        if ($idade < 18) { alert("menina") }

        else {
            alert("Mulher")
        }
    }




</script>

